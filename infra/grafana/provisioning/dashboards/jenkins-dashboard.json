{
  "id": null,
  "uid": "jenkins-monitoring",
  "title": "Jenkins Monitoring",
  "tags": ["jenkins"],
  "timezone": "browser",
  "schemaVersion": 27,
  "version": 0,
  "panels": [
    {
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "targets": [
        {
          "expr": "jenkins_builds_success_total",
          "legendFormat": "Successful Builds",
          "refId": "A"
        },
        {
          "expr": "jenkins_builds_failed_total",
          "legendFormat": "Failed Builds",
          "refId": "B"
        }
      ],
      "title": "Jenkins Build Status",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "id": 2,
      "targets": [
        {
          "expr": "jenkins_queue_size_value",
          "legendFormat": "Queue Size",
          "refId": "A"
        }
      ],
      "title": "Jenkins Queue Size",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "id": 3,
      "targets": [
        {
          "expr": "jenkins_builds_duration_milliseconds_summary{quantile=\"0.5\"} / 1000",
          "legendFormat": "Median Build Duration",
          "refId": "A"
        },
        {
          "expr": "jenkins_builds_duration_milliseconds_summary{quantile=\"0.95\"} / 1000",
          "legendFormat": "95th Percentile",
          "refId": "B"
        }
      ],
      "title": "Jenkins Build Duration",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "id": 4,
      "targets": [
        {
          "expr": "jenkins_executor_count_value",
          "legendFormat": "Total Executors",
          "refId": "A"
        },
        {
          "expr": "jenkins_executor_in_use_value",
          "legendFormat": "Busy Executors",
          "refId": "B"
        }
      ],
      "title": "Jenkins Executors",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 16
      },
      "id": 5,
      "targets": [
        {
          "expr": "jenkins_vm_memory_heap_used",
          "legendFormat": "Heap Used",
          "refId": "A"
        },
        {
          "expr": "jenkins_vm_memory_heap_max",
          "legendFormat": "Heap Max",
          "refId": "B"
        }
      ],
      "title": "Jenkins JVM Memory",
      "type": "timeseries"
    }
  ],
  "time": {
    "from": "now-30m",
    "to": "now"
  },
  "refresh": "10s",
  "templating": {
    "list": []
  },
  "annotations": {
    "list": []
  }
}