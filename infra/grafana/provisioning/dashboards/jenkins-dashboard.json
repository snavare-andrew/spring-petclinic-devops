{
  "id": null,
  "uid": "jenkins-monitoring",
  "title": "Jenkins Monitoring",
  "tags": ["jenkins"],
  "timezone": "browser",
  "schemaVersion": 27,
  "version": 0,
  "panels": [
    {
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "targets": [
        {
          "expr": "default_jenkins_builds_success_build_count_total",
          "legendFormat": "{{jenkins_job}} - Success",
          "refId": "A"
        },
        {
          "expr": "default_jenkins_builds_total_build_count_total - default_jenkins_builds_success_build_count_total",
          "legendFormat": "{{jenkins_job}} - Failed",
          "refId": "B"
        }
      ],
      "title": "Jenkins Build Counts",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "id": 2,
      "targets": [
        {
          "expr": "jenkins_queue_size_value",
          "legendFormat": "Queue Size",
          "refId": "A"
        }
      ],
      "title": "Jenkins Queue Size",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "id": 3,
      "targets": [
        {
          "expr": "default_jenkins_builds_last_build_duration_milliseconds",
          "legendFormat": "{{jenkins_job}}",
          "refId": "A"
        }
      ],
      "title": "Jenkins Last Build Duration",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "id": 4,
      "targets": [
        {
          "expr": "jenkins_executor_count_value",
          "legendFormat": "Total Executors",
          "refId": "A"
        },
        {
          "expr": "jenkins_executor_in_use_value",
          "legendFormat": "Busy Executors",
          "refId": "B"
        },
        {
          "expr": "jenkins_executor_free_value",
          "legendFormat": "Free Executors",
          "refId": "C"
        }
      ],
      "title": "Jenkins Executors",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 16
      },
      "id": 5,
      "targets": [
        {
          "expr": "jvm_memory_bytes_used{area=\"heap\"}",
          "legendFormat": "Heap Used",
          "refId": "A"
        },
        {
          "expr": "jvm_memory_bytes_max{area=\"heap\"}",
          "legendFormat": "Heap Max",
          "refId": "B"
        }
      ],
      "title": "Jenkins JVM Heap Memory",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 16
      },
      "id": 6,
      "targets": [
        {
          "expr": "default_jenkins_builds_health_score",
          "legendFormat": "{{jenkins_job}} - Health Score",
          "refId": "A"
        }
      ],
      "title": "Jenkins Job Health Score",
      "type": "timeseries"
    }
  ],
  "time": {
    "from": "now-30m",
    "to": "now"
  },
  "refresh": "10s",
  "templating": {
    "list": []
  },
  "annotations": {
    "list": []
  }
}